# AOS-CX Switch Connection Configuration
# Copy this file to .env and update with your actual values

# Required: Switch hostname or IP address
AOSCX_HOSTNAME=192.168.1.100

# Required: Switch password
AOSCX_PASSWORD=your_password_here

# Optional: Switch username (defaults to 'admin')
AOSCX_USERNAME=admin

# Optional: API version (defaults to 'v10.09')
AOSCX_VERSION=v10.09

# Optional: Certificate verification (defaults to false)
# Set to 'true' for production environments
AOSCX_VERIFY_CERT=false 